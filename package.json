  "name": "cloudportal",
  "private": true,
  "scripts": {
    "build": "yarn vue-cli build",
    "build-storybook": "build-storybook",
    "build:analyzer": "cross-env BUNDLE_ANALYZER=true npm run build",
    "build:dev": "yarn vue-cli build --mode=development",
    "build:serve": "serve -s dist",
    "cypress": "cypress run",
    "cypress-ui": "cypress open",
    "dev": "yarn vue-cli serve --open",
    "lint": "yarn vue-cli lint",
    "lint:ci": "yarn vue-cli lint --no-fix --max-warnings=0 --max-errors=0",
    "merge-report": "jrm \"./cypress/results/mergeReports.xml\" \"./cypress/results/*.xml\"",
    "serve": "yarn vue-cli serve",
    "storybook": "start-storybook -p 6006",
    "test:cover": "cross-env NODE_ENV=coverage nyc --nycrc-path tests/unit/.nycrc.json yarn vue-cli test:unit -r ts-node/register/transpile-only -r tests/unit/setup.js -c \"src/**/*.spec.{js,ts}\" --exit",
    "test:e2e": "yarn vue-cli test:e2e --mode development --browser chrome",
    "test:e2e:ci": "cross-env NODE_ENV=ci yarn vue-cli test:e2e --mode development --headless",
    "test:unit": "yarn vue-cli test:unit --opts tests/unit/mocha.opts -r tests/unit/setup.js -w \"{src,tests/unit}/**/*.spec.{js,ts}\"",
    "test:unit:ci": "cross-env NODE_ENV=ci yarn vue-cli test:unit --opts tests/unit/mocha.opts -r tests/unit/setup.js \"{src,tests/unit}/**/*.spec.{js,ts}\" --exit",
    "vue-cli": "NODE_OPTIONS='--openssl-legacy-provider' vue-cli-service",
    "webpack:inspect": "yarn vue-cli inspect > webpack.inspect.config.js"
  },
